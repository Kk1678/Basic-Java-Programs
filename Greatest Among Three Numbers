import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner z = new Scanner(System.in);
      int a = z.nextInt();
      int b = z.nextInt();
      int c = z.nextInt();
      if(a>b && a>c){
        System.out.println(a);
      }else if (b>a && b>c){
        System.out.println(b);
      }else {
      System.out.println(c);
  }
  }
}
