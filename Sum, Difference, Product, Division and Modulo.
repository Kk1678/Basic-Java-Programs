import java.util.*;

public class Main {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int A = sc.nextInt();
       int B = sc.nextInt();
       int add = A + B;
       int sub = A - B;
       int mul = A * B;
       int quo = A / B;
       int rem = A % B;
      System.out.println(add);
      System.out.println(sub);
      System.out.println(mul);
      System.out.println(quo);
      System.out.println(rem);
  }
}
